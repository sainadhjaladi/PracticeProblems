/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		for(int i=0;i<num;i++)
		{
		    String str1 = sc.next();
		    String str2 = sc.next();
		    int len = str1.length();
		    for(int j=0;j<len;j++)
		    {
		        if(str1.charAt(j)==str2.charAt(j))
		        {
		            System.out.print("G");
		        }
		        else
		        {
		            System.out.print("B");
		        }
		    }
		    System.out.println();
		}
	}
}
